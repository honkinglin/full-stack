<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.office.oa.mapper.DepartmentMapper">
    <select id="selectById" parameterType="Long" resultType="com.office.oa.entity.Department">
        select * from adm_department where department_id = #{value}
    </select>
</mapper>